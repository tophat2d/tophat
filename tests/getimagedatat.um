
import (
    "image.um"
    "signal.um"
    "window.um"
		"std.um"
)

var (
    img: image::Image
)

fn init*() {
	var err: std::Err

	window::setup("get image data test", 640, 480)

	img, err = image::load("etc/test.png")
	std::exitif(err)

	d := img.getData()

	for i in d {
		printf("%x\n", d[i])
	}

	exit(0)
}
